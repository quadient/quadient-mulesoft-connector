{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Content Author - Templates Operation",
  "type": "object",
  "properties": {
    "templates": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "folder": {
            "type": "string"
          },
          "version": {
            "type": "number"
          },
          "edited": {
            "type": "boolean"
          },
          "metadata": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "values": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                }
              },
              "required": [
                "name",
                "values"
              ]
            }
          }
        },
        "required": [
          "name",
          "folder",
          "version",
          "edited",
          "metadata"
        ]
      }
    },
    "totalCount": {
      "type": "number"
    }
  },
  "required": [
    "templates",
    "totalCount"
  ]
}